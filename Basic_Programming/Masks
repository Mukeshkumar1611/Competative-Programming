#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--)
	{
	    int a, b;
	    cin >> a >> b;
	    if(a * 100 < b * 10)
	    {
	        cout << "Disposable" << endl;
	    }
	    else
	    {
	        cout << "CLoth" << endl;
	    }
	}
	return 0;
}
