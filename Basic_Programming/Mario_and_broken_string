#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin >> t;
	while(t--)
	{
	    int n;
	    cin >> n;
	    string str;
	    cin >> str;
	    
	    string a = "";
	    string b = "";
	    
	    for(int i=0; i< (n/2); i++)
	    {
	        a += str[i];
	    }
	    
	    for(int i=(n/2); i<n;i++)
	    {
	        b += str[i];
	    }
	    
	    if(a == b)
	    {
	        cout << "YES" << endl;
	    }
	    else{
	        cout << "NO" << endl;
	    }
	}
	return 0;
}
