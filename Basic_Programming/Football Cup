#include <iostream>
using namespace std;

int main() {
	// your code goes here
	
	int t;
	cin >> t;
	while(t--)
	{
	    int a,b;
	    cin >> a >> b;
	    if(a > 0 and b > 0 and a==b)
	    {
	        cout << "YES" << endl;
	        
	    }
	    else{
	        cout << "NO" << endl;
	    }
	}
	return 0;
}
